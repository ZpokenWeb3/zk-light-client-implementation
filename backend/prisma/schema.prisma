// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model BlockProof {
  hash      String @id @default("")
  epochId   String
  height    Int
  timestamp String
  status   String
  hexInput String
  previousEpochHash String
  nextBlockHash String
  dateCreate String
}

